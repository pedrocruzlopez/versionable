{
    "name": "kenny08gt/versionable",
    "license": "MIT",
    "description": "Fork from mpociot/versionable Allows to create Laravel 4 / 5 / 6 / 7 Model versioning and restoring",
    "keywords": [
        "model",
        "laravel",
        "ardent",
        "version",
        "history",
        "restore"
    ],
    "homepage": "https://github.com/kenny08gt/versionable",
    "authors": [
        {
            "name": "Alan Hurtarte",
            "email": "alan.hurtarte@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/kenny08gt/versionable/issues",
        "source": "https://github.com/kenny08gt/versionable"
    },
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "~5.3 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.1 || ^4.0 || ^5.0 || ^6.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "tests/VersionableTestCase.php"
        ],
        "psr-0": {
            "Kenny08gt\\Versionable": "src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kenny08gt\\Versionable\\Providers\\ServiceProvider"
            ]
        }
    }
}
